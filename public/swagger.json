openapi: 3.0.3
info:
  title: ShipexIndia API Guide
  version: 1.0.0
  contact: {}
servers:
  - url: https://api.shipexindia.com
paths:
  /v1/api/external/createOrder:
    post:
      summary: OrderCreation
      description: >-
        **Endpoint:**  

        `POST https://api.shipexindia.com/v1/api/external/createOrder`


        **Description:**


        This API is used to **create a new shipment order**. It requires sender
        (pickup) details, receiver details, product information, package
        dimensions, and payment details. A valid JWT token (from the Generate
        Token API) must be passed in the request header.


        **Headers:**


        - `Content-Type: application/json`
            
        - `Authorization: Bearer`
            

        Response (Success 200):


        {  

        "success": true,  

        "message": "Order created successfully.",  

        "data": {  

        "orderId": 566281,  

        "clientOrderId": 8984810,  

        "status": "new"  

        }  

        }


        400 Bad Request


        {  

        "success": false,  

        "message": "Validation error",  

        "details": "Phone number must be exactly 10 digits"  

        }


        401 Unauthorized


        {  

        "success": false,  

        "message": "Invalid or expired token"  

        }
      operationId: ordercreation
      requestBody:
        content:
          application/json:
            examples:
              OrderCreation:
                value: "{\r\n    \"shipmentId\":8984810,\r\n  \"pickupAddress\": {\r\n    \"contactName\": \"John Doe\",\r\n    \"email\": \"john@example.com\",\r\n    \"phoneNumber\": \"9876543201\",\r\n    \"address\": \"123 Main St\",\r\n    \"pinCode\": \"560002\",\r\n    \"city\": \"Bangalore\",\r\n    \"state\": \"Karnataka\"\r\n  },\r\n  \"receiverAddress\": {\r\n    \"contactName\": \"Jane Smith\",\r\n    \"email\": \"jane@example.com\",\r\n    \"phoneNumber\": \"9123456781\",\r\n    \"address\": \"456 Market Ave\",\r\n    \"pinCode\": \"400002\",\r\n    \"city\": \"Mumbai\",\r\n    \"state\": \"Maharashtra\"\r\n  },\r\n  \"productDetails\": [\r\n    {\r\n      \"id\": \"1\",\r\n      \"quantity\": 2,\r\n      \"name\": \"T-Shirt\",\r\n      \"sku\": \"TSHIRT123\",\r\n      \"unitPrice\": \"499\"\r\n    }\r\n  ],\r\n  \"packageDetails\": {\r\n    \"deadWeight\": 0.5,//\"KG\"\r\n    \"applicableWeight\": 0.6,//\"KG\"\r\n    \"volumetricWeight\": {\r\n      \"length\": 10,//\"cm\"\r\n      \"width\": 15,//\"cm\"\r\n      \"height\": 5,//\"cm\"\r\n      \"calculatedWeight\": 0.6//\"KG\"\r\n    }\r\n  },\r\n  \"paymentDetails\": {\r\n    \"method\": \"COD\",//[\"COD\",\"Prepaid\"]\r\n    \"amount\": 998\r\n  }\r\n}\r\n"
      responses:
        '201':
          description: OrderCreation
          headers:
            Access-Control-Allow-Origin:
              schema:
                type: string
                example: '*'
            Connection:
              schema:
                type: string
                example: keep-alive
            Content-Length:
              schema:
                type: string
                example: '121'
            Content-Security-Policy:
              schema:
                type: string
                example: >-
                  default-src 'self';base-uri 'self';font-src 'self' https:
                  data:;form-action 'self';frame-ancestors 'self';img-src 'self'
                  data:;object-src 'none';script-src 'self';script-src-attr
                  'none';style-src 'self' https:
                  'unsafe-inline';upgrade-insecure-requests
            Cross-Origin-Opener-Policy:
              schema:
                type: string
                example: same-origin
            Cross-Origin-Resource-Policy:
              schema:
                type: string
                example: same-origin
            Date:
              schema:
                type: string
                example: Mon, 25 Aug 2025 13:06:35 GMT
            ETag:
              schema:
                type: string
                example: W/"79-66RKSwcCU6KqGolwteHMFQB77BY"
            Origin-Agent-Cluster:
              schema:
                type: string
                example: '?1'
            Referrer-Policy:
              schema:
                type: string
                example: no-referrer
            Server:
              schema:
                type: string
                example: nginx/1.26.3
            Strict-Transport-Security:
              schema:
                type: string
                example: max-age=31536000; includeSubDomains
            Vary:
              schema:
                type: string
                example: Accept-Encoding
            X-Content-Type-Options:
              schema:
                type: string
                example: nosniff
            X-DNS-Prefetch-Control:
              schema:
                type: string
                example: 'off'
            X-Download-Options:
              schema:
                type: string
                example: noopen
            X-Frame-Options:
              schema:
                type: string
                example: SAMEORIGIN
            X-Permitted-Cross-Domain-Policies:
              schema:
                type: string
                example: none
            X-XSS-Protection:
              schema:
                type: string
                example: '0'
          content:
            application/json:
              schema:
                type: object
                properties:
                  data:
                    type: object
                    properties:
                      clientOrderId:
                        type: number
                        example: 8984810
                      orderId:
                        type: number
                        example: 485888
                      status:
                        type: string
                        example: new
                  message:
                    type: string
                    example: Order created successfully.
                  success:
                    type: boolean
                    example: true
              examples:
                OrderCreation:
                  value:
                    data:
                      clientOrderId: 8984810
                      orderId: 485888
                      status: new
                    message: Order created successfully.
                    success: true
  /v1/api/external/generateToken:
    post:
      summary: Generate Token
      description: >-
        **Endpoint:**  

        `POST https://api.shipexindia.com/v1/api/external/generateToken`


        **Description:**


        This API authenticates a user using their email and password, and
        returns a **JWT token**. The token must be included in the
        `Authorization` header (`Bearer` ) for all further requests.


        **Headers:**


        - `Content-Type: application/json`
            

        Request Body:


        {  

        "email": "[test@gmail.com](https://mailto:test@gmail.com)",  

        "password": "test@1212"  

        }


        Response (Success 200):


        {  

        "success": true,  

        "message": "Token generated successfully",  

        "data": {  

        "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6..."  

        }  

        }


        Error Responses:


        - 401 Unauthorized
            

        {  

        "success": false,  

        "message": "Invalid email or password"  

        }


        - 500 Internal Server Error
            

        {  

        "success": false,  

        "message": "Something went wrong"  

        }
      operationId: generateToken
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                email:
                  type: string
                  example: test@gmail.com
                password:
                  type: string
                  example: test@1010
            examples:
              Generate Token:
                value:
                  email: test@gmail.com
                  password: test@1010
      responses:
        '200':
          description: Generate Token
          headers:
            Access-Control-Allow-Origin:
              schema:
                type: string
                example: '*'
            Connection:
              schema:
                type: string
                example: keep-alive
            Content-Length:
              schema:
                type: string
                example: '412'
            Content-Security-Policy:
              schema:
                type: string
                example: >-
                  default-src 'self';base-uri 'self';font-src 'self' https:
                  data:;form-action 'self';frame-ancestors 'self';img-src 'self'
                  data:;object-src 'none';script-src 'self';script-src-attr
                  'none';style-src 'self' https:
                  'unsafe-inline';upgrade-insecure-requests
            Cross-Origin-Opener-Policy:
              schema:
                type: string
                example: same-origin
            Cross-Origin-Resource-Policy:
              schema:
                type: string
                example: same-origin
            Date:
              schema:
                type: string
                example: Mon, 25 Aug 2025 08:09:23 GMT
            ETag:
              schema:
                type: string
                example: W/"19c-K42LzenfK9/n5lHa9f1EyacDdxw"
            Origin-Agent-Cluster:
              schema:
                type: string
                example: '?1'
            Referrer-Policy:
