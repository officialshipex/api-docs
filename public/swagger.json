        "courierServiceName": "Delhivery Surface",  

        "courierId": "02",  

        "awb_number": "DEL123456789",  

        "labelUrl":
        "[https://shipx.s3.amazonaws.com/labels/DEL123456789.pdf"](https://shipx.s3.amazonaws.com/labels/DEL123456789.pdf)  

        }  

        }


