openapi: 3.0.0
info:
  title: ShipexIndia API Guide
  version: 1.0.0
servers:
  - url: https://api.shipexindia.com
paths:
  /v1/api/external/createOrder:
    post:
      tags:
        - default
      summary: Create Order
      description: >-
        **Endpoint:**  

        `POST https://api.shipexindia.com/v1/api/external/createOrder`


        **Description:**


        This API is used to **create a new shipment order**. It requires sender
        (pickup) details, receiver details, product information, package
        dimensions, and payment details. A valid JWT token (from the Generate
        Token API) must be passed in the request header.


        **Headers:**


        - `Content-Type: application/json`
            
        - `Authorization: Bearer`
            

        Response (Success 200):


        {  

        "success": true,  

        "message": "Order created successfully.",  

        "data": {  

        "orderId": 566281,  

        "clientOrderId": 8984810,  

        "status": "new"  

        }  

        }


        400 Bad Request


        {  

        "success": false,  

        "message": "Validation error",  

        "details": "Phone number must be exactly 10 digits"  

        }


        401 Unauthorized


        {  

        "success": false,  

        "message": "Invalid or expired token"  

        }
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                shipmentId: 8984810
                pickupAddress:
                  contactName: John Doe
                  email: john@example.com
                  phoneNumber: '9876543201'
                  address: 123 Main St
                  pinCode: '560002'
                  city: Bangalore
                  state: Karnataka
                receiverAddress:
                  contactName: Jane Smith
                  email: jane@example.com
                  phoneNumber: '9123456781'
                  address: 456 Market Ave
                  pinCode: '400002'
                  city: Mumbai
                  state: Maharashtra
                productDetails:
                  - id: '1'
                    quantity: 2
                    name: T-Shirt
                    sku: TSHIRT123
                    unitPrice: '499'
                packageDetails:
                  deadWeight: 0.5
                  applicableWeight: 0.6
                  volumetricWeight:
                    length: 10
                    width: 15
                    height: 5
                    calculatedWeight: 0.6
                paymentDetails:
                  method: COD
                  amount: 998
      parameters:
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
        - name: Authorization
          in: header
          schema:
            type: string
          example: >-
            Bearer
            eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJ1c2VyIjp7ImlkIjoiNjg3N2NkODE0YjEzMGIzZjI1YTY0NzExIiwiZW1haWwiOiJiaGFuamFiaWpheWtldGFuQGdtYWlsLmNvbSIsImZ1bGxuYW1lIjoiQmlqYXkga2V0YW4gQmhhbmphIiwia3ljIjp0cnVlLCJpc0FkbWluIjp0cnVlLCJpc0VtcGxveWVlIjpmYWxzZX0sImlhdCI6MTc1NjEwOTM2MywiZXhwIjoxNzU2MTk1NzYzfQ.vfbrr_f0i7tifdTkAw2LHp3LaORQkOMsGfXI-GEiAK0
      responses:
        '201':
          description: Created
          headers:
            Server:
              schema:
                type: string
                example: nginx/1.26.3
            Date:
              schema:
                type: string
                example: Mon, 25 Aug 2025 13:06:35 GMT
            Content-Type:
              schema:
                type: string
                example: application/json; charset=utf-8
            Content-Length:
              schema:
                type: integer
                example: '121'
            Connection:
              schema:
                type: string
                example: keep-alive
            Content-Security-Policy:
              schema:
                type: string
                example: >-
                  default-src 'self';base-uri 'self';font-src 'self' https:
                  data:;form-action 'self';frame-ancestors 'self';img-src 'self'
                  data:;object-src 'none';script-src 'self';script-src-attr
                  'none';style-src 'self' https:
                  'unsafe-inline';upgrade-insecure-requests
            Cross-Origin-Opener-Policy:
              schema:
                type: string
                example: same-origin
            Cross-Origin-Resource-Policy:
              schema:
                type: string
                example: same-origin
            Origin-Agent-Cluster:
              schema:
                type: string
                example: '?1'
            Referrer-Policy:
              schema:
                type: string
                example: no-referrer
            Strict-Transport-Security:
              schema:
                type: string
                example: max-age=31536000; includeSubDomains
            X-Content-Type-Options:
              schema:
                type: string
                example: nosniff
            X-DNS-Prefetch-Control:
              schema:
                type: string
                example: 'off'
            X-Download-Options:
              schema:
                type: string
                example: noopen
            X-Frame-Options:
              schema:
                type: string
                example: SAMEORIGIN
            X-Permitted-Cross-Domain-Policies:
              schema:
                type: string
                example: none
            X-XSS-Protection:
              schema:
                type: integer
                example: '0'
            Access-Control-Allow-Origin:
              schema:
                type: string
                example: '*'
            ETag:
              schema:
                type: string
                example: W/"79-66RKSwcCU6KqGolwteHMFQB77BY"
            Vary:
              schema:
                type: string
                example: Accept-Encoding
          content:
            text/plain:
              schema:
                type: string
              example: |-
                {
                    "success": true,
                    "message": "Order created successfully.",
                    "data": {
                        "orderId": 485888,
                        "clientOrderId": 8984810,
                        "status": "new"
                    }
                }
  /v1/api/external/generateToken:
    post:
      tags:
        - default
      summary: Generate Token
      description: >-
        **Endpoint:**  

        `POST https://api.shipexindia.com/v1/api/external/generateToken`


        **Description:**


        This API authenticates a user using their email and password, and
        returns a **JWT token**. The token must be included in the
        `Authorization` header (`Bearer` ) for all further requests.


        **Headers:**


        - `Content-Type: application/json`
            

        Request Body:


        {  

        "email": "[test@gmail.com](https://mailto:test@gmail.com)",  

        "password": "test@1212"  

        }


        Response (Success 200):


        {  

        "success": true,  

        "message": "Token generated successfully",  

        "data": {  

        "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6..."  

        }  

        }


        Error Responses:


        - 401 Unauthorized
            

        {  

        "success": false,  

        "message": "Invalid email or password"  

        }


        - 500 Internal Server Error
            

        {  

        "success": false,  

        "message": "Something went wrong"  

        }
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                email: test@gmail.com
                password: Test@1234
      parameters:
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
      responses:
        '200':
          description: OK
          headers:
            Server:
              schema:
                type: string
                example: nginx/1.26.3
            Date:
              schema:
                type: string
                example: Mon, 25 Aug 2025 08:09:23 GMT
            Content-Type:
              schema:
                type: string
                example: application/json; charset=utf-8
            Content-Length:
              schema:
                type: integer
                example: '412'
            Connection:
              schema:
                type: string
                example: keep-alive
            Content-Security-Policy:
              schema:
                type: string
                example: >-
                  default-src 'self';base-uri 'self';font-src 'self' https:
                  data:;form-action 'self';frame-ancestors 'self';img-src 'self'
                  data:;object-src 'none';script-src 'self';script-src-attr
                  'none';style-src 'self' https:
                  'unsafe-inline';upgrade-insecure-requests
            Cross-Origin-Opener-Policy:
              schema:
                type: string
                example: same-origin
            Cross-Origin-Resource-Policy:
              schema:
                type: string
                example: same-origin
            Origin-Agent-Cluster:
              schema:
                type: string
                example: '?1'
            Referrer-Policy:
              schema:
                type: string
                example: no-referrer
            Strict-Transport-Security:
              schema:
                type: string
                example: max-age=31536000; includeSubDomains
            X-Content-Type-Options:
              schema:
                type: string
                example: nosniff
            X-DNS-Prefetch-Control:
              schema:
                type: string
                example: 'off'
            X-Download-Options:
              schema:
                type: string
                example: noopen
            X-Frame-Options:
              schema:
                type: string
                example: SAMEORIGIN
            X-Permitted-Cross-Domain-Policies:
              schema:
